            div.d-fz-14.line-h-120.mob-fz-12= characteristic.label
            div.d-fz-18.line-h-120.mob-fz-16= characteristic.value
      .characteristics__img1
        .characteristics__img1-wrap(data-paralax-wrap)
          img(src="./assets/images/characteristics/img1.jpg", alt="", srcset="" loading="lazy" )
